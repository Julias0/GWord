{
    "manifest_version": 2,
    "name": "Super Extension",
    "version": "0.1",
    "content_scripts": [
        {
            "matches":["https://mail.google.com/mail/*"],
            "js": [
                "jquery.min.js",
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}